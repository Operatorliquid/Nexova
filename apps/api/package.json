{
  "name": "@nexova/api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/main.js",
  "scripts": {
    "build": "prisma generate --schema ../../prisma/schema.prisma && tsc --build",
    "clean": "rm -rf dist *.tsbuildinfo",
    "dev": "tsx watch src/main.ts",
    "start": "node dist/main.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.0",
    "@fastify/cookie": "^9.0.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/multipart": "^8.3.1",
    "@fastify/rate-limit": "^9.0.0",
    "@fastify/static": "^7.0.4",
    "@fastify/websocket": "^10.0.0",
    "@nexova/core": "workspace:*",
    "@nexova/integrations": "workspace:*",
    "@nexova/shared": "workspace:*",
    "@prisma/client": "^5.22.0",
    "bullmq": "^5.4.0",
    "dotenv": "^17.3.0",
    "fastify": "^4.28.0",
    "fastify-plugin": "^4.5.0",
    "ioredis": "^5.4.1",
    "nodemailer": "^8.0.1",
    "prisma": "^5.10.0",
    "stripe": "^20.3.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/nodemailer": "^7.0.9",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "vitest": "^1.3.0"
  }
}

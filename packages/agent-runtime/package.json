{
  "name": "@nexova/agent-runtime",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "import": "./dist/tools/index.js"
    },
    "./worker": {
      "types": "./dist/worker/agent-worker.d.ts",
      "import": "./dist/worker/agent-worker.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run --passWithNoTests",
    "test:unit": "vitest run --dir test/unit",
    "worker": "tsx src/worker/start.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@prisma/client": "^5.22.0",
    "@nexova/shared": "workspace:*",
    "@nexova/core": "workspace:*",
    "@nexova/integrations": "workspace:*",
    "tesseract.js": "^5.0.6",
    "bullmq": "^5.52.1",
    "ioredis": "^5.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "vitest": "^1.3.0",
    "tsx": "^4.19.0"
  }
}
